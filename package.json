{
	"name": "rtl-comment-support",
	"displayName": "RTL Comment Support",
	"description": "With this extension you can leave comments for your code in right-to-left languages.",
	"version": "1.0.0",
	"publisher": "rouhollah-torshizi",
	"license": "GPL-2.0 License",
	"engines": {
		"vscode": "^1.49.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:rtl-comment-support"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "rtl-comment-support",
				"title": "RTL Comment"
			}
		],
		"keybindings": [
			{
				"command": "rtl-comment-support",
				"key": "F6"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.49.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.0",
		"@types/node": "^12.11.7",
		"eslint": "^7.9.0",
		"@typescript-eslint/eslint-plugin": "^4.1.1",
		"@typescript-eslint/parser": "^4.1.1",
		"glob": "^7.1.6",
		"mocha": "^8.1.3",
		"typescript": "^4.0.2",
		"vscode-test": "^1.4.0"
	},
	"dependencies": {
		"yarn": "^1.22.10"
	},
	"bugs": {
		"url": "https://github.com/Rouhollah/rtl-comment-support/issues",
		"email": "torshizi1@gmail.com"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/Rouhollah/rtl-comment-support"
	},
	"homepage": "https://github.com/Rouhollah/rtl-comment-support/blob/master/README.md",
	"icon": "icon.png"
}